{
    "name": "imarc/craft-events",
    "description": "A plugin for managing events in Craft",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft events"
    ],
    "support": {
        "docs": "https://github.com/imarc/craft-events/blob/master/README.md",
        "issues": "https://github.com/imarc/craft-events/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Imarc",
            "homepage": "https://www.imarc.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "imarc\\craft\\events\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Events",
        "handle": "craft-events",
        "developer": "Imarc",
        "developerUrl": "https://www.imarc.com",
        "documentationUrl": "https://github.com/imarc/craft-events/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/imarc/craft-events/master/CHANGELOG.md",
        "components": {
            "events": "imarc\\craft\\events\\services\\Events"
        },
        "class": "imarc\\craft\\events\\CraftEvents"
    }
}